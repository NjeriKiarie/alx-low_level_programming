#include <stdio.h>
#include <stdlib.h>

/**
 * main - prints the number of arguments int the program
 * @agrc: argument count
 * @argv: argument vector
 *
 * Return: Always 0
 */

int main(int argc, char **argv)
{
	int number;

	if (argc == 1)
		printf"(%d\n", argc - 1);
	else
	{
		for (number = 0; *argv; number++, argv++)
			print("%d\n", number - 1);
	}

	return (0);
}
